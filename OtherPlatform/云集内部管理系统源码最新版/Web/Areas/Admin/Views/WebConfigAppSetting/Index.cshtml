@model System.Collections.Specialized.NameValueCollection

<table class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th sort="decrip">
                名称
            </th>
            <th sort="decrip">
                值
            </th>
            <th>
                @lang.Edit
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (string item in Model.Keys)
        {
            <tr>
                <td>
                    @item
                </td>
                <td>
                    @Model[item]
                </td>
                <td>
                    <div>
                        @Ajax.ActionLink(lang.Edit, "Edit", new { id = item }, new AjaxOptions { UpdateTargetId = "Main", HttpMethod = "Get" })
                    </div>
                </td>
            </tr>
        }
    </tbody>
   
</table>
